 
  <h2>Users</h2>

<table>
    <thead>
      <tr>
        <th>image</th>
        <th>name</th>
        <th></th>
        <th colspan="3"></th>
      </tr>
    </thead>
    <% @users.each do |book| %>
    <tbody>
      <tr>
        <td><%= attachment_image_tag @user, :profile_image, :fill, 60, 60, format: 'jpeg', fallback: "https://via.placeholder.com/60x60" %></td>
        <td><%= book.user.name %></td>
        <td><%= link_to "Show", edit_user_path(book.user.id) %></td>
      </tr>
    </tbody>
  </table>
<% end %>

<%= paginate @users%>